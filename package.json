{
  "name": "puppeteer-fetch",
  "version": "0.1.5",
  "description": "Fetch with Puppeteer, just like node-fetch, but use puppeteer as the driver",
  "engines": {
    "node": ">= 14.15 <15"
  },
  "main": "build/src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/theowenyoung/puppeteer-fetch"
  },
  "keywords": [
    "puppeteer",
    "fetch",
    "scrape",
    "waf",
    "axios",
    "http",
    "request"
  ],
  "devDependencies": {
    "@types/jest": "~26.0.19",
    "@types/node": "~14.14.20",
    "@typescript-eslint/eslint-plugin": "~4.12.0",
    "@typescript-eslint/parser": "~4.12.0",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "eslint": "~7.17.0",
    "eslint-config-prettier": "~7.1.0",
    "eslint-plugin-jest": "~24.1.3",
    "jest": "~26.6.3",
    "prettier": "~2.2.1",
    "release-it": "^14.4.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~26.4.4",
    "tsutils": "~3.18.0",
    "typescript": "~4.1.3"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "del build/",
    "build": "tsc -p tsconfig.release.json",
    "postbuild": "cpy '**/*' '!**/*.ts' ../build/src/ --cwd=src/ --no-overwrite --parents",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "release": "release-it"
  },
  "author": "Owen Young <theowenyoung@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "handlebars": "^4.7.7",
    "puppeteer": "^7.1.0",
    "tslib": "~2.0.3"
  },
  "volta": {
    "node": "14.15.0"
  }
}
